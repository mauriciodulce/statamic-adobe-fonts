{
    "name": "dulce/statamic-adobe-fonts",
    "description": "Manage self-hosted Adobe Typekit Fonts in Statamic",
    "type": "statamic-addon",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dulce\\StatamicAdobeFonts\\": "src"
        }
    },
    "require": {
        "php": "^8.1",
        "statamic/cms": "^4.0",
        "illuminate/contracts": "^10.0",
        "illuminate/support": "^10.0",
        "spatie/laravel-package-tools": "^1.16.0"
    },
    "extra": {
        "statamic": {
            "name": "Statamic Adobe Fonts",
            "description": "Statamic Adobe Fonts addon"
        },
        "laravel": {
            "providers": [
                "Dulce\\StatamicAdobeFonts\\ServiceProvider"
            ]
        }
    }
}
